    "build": "./node_modules/.bin/babel --optional reactCompat --stage 1 ./src --out-dir ./lib",
    "prepublish": "npm run build"
  },
  "keywords": [
    "react",
    "react-component",
    "material design",
    "material-ui",
    "material ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/br0r/material-ui.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/br0r/material-ui/issues"
  },
  "dependencies": {
    "gulp": "^3.8.11",
    "react-classset": "0.0.2",
    "react-draggable2": "^0.4.2"
  },
  "peerDependencies": {
    "react": "0.12.x",
    "react-tap-event-plugin": "^0.1.3"
  },
  "devDependencies": {
    "autoprefixer-core": "^5.1.8",
    "babel": "^5.4.7",
    "browserify": "^7.0.3",
    "gulp-postcss": "^5.1.0",
    "gulp-sourcemaps": "^1.5.1",
    "jsxhint": "^0.15.0",
    "react-router": "^0.11.6",
    "react-tools": "^0.12.2",
    "reactify": "^0.17.1"
  },
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "stripTypes": true,
          "es6": true,
          "harmony": true
        }
      ]
    ]
  }
